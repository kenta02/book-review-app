{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "ts-node --transpileOnly ./src/server.ts",
    "swagger": "ts-node --transpileOnly ./src/swagger-server.ts",
    "token": "ts-node ./scripts/get-token.ts",
    "db:migrate": "ts-node ./scripts/run-migrations.ts up",
    "db:migrate:down": "ts-node ./scripts/run-migrations.ts down",
    "db:migrate:status": "ts-node ./scripts/run-migrations.ts status",
    "db:migrate:pending": "ts-node ./scripts/run-migrations.ts pending",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ci": "vitest --run",
    "coverage": "vitest --coverage",
    "coverage:ci": "vitest --run --coverage",
    "lint": "eslint 'src/**/*.{ts,tsx}' --ext .ts",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --ext .ts --fix",
    "format": "prettier --write .",
    "check-types": "tsc -p tsconfig.json --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "6.0.0",
    "cors": "2.8.5",
    "dotenv": "17.2.3",
    "express": "5.1.0",
    "jsonwebtoken": "9.0.2",
    "mysql2": "3.15.1",
    "sequelize": "6.37.7",
    "swagger-ui-express": "5.0.0",
    "yaml": "^2.4.1"
  },
  "devDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.57.1",
    "@types/bcrypt": "6.0.0",
    "@types/cors": "2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "9.0.10",
    "@types/node": "24.10.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitest/coverage-v8": "^1.6.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-unused-imports": "^3.1.0",
    "nodemon": "3.1.10",
    "prettier": "^3.8.0",
    "supertest": "^6.3.3",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "typescript": "5.9.3",
    "umzug": "^3.1.0",
    "vitest": "^1.5.7"
  }
}
